{
    "version": 2,
    "builds": [
      {
        "src": "Host.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest":  "/Host.js"
      }
    ],
    "headers": [
      {
        "source": "/logout",
        "headers": [
          { "key": "Cache-Control", "value": "no-store, max-age=0" },
          { "key": "Pragma", "value": "no-cache" },
          { "key": "Expires", "value": "0" }
        ]
      },
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "no-store, max-age=0" },
          { "key": "Pragma", "value": "no-cache" },
          { "key": "Expires", "value": "0" }
        ]
      }
    ]
  }